{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vaults_keyvaultenv_name": {
            "defaultValue": "keyvaultenv",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.KeyVault/vaults",
            "apiVersion": "2024-04-01-preview",
            "name": "[parameters('vaults_keyvaultenv_name')]",
            "location": "canadacentral",
            "properties": {
                "sku": {
                    "family": "A",
                    "name": "Standard"
                },
                "tenantId": "d7ff8c91-df8c-4499-ad6b-a775c5e24ecb",
                "accessPolicies": [],
                "enabledForDeployment": true,
                "enabledForDiskEncryption": false,
                "enabledForTemplateDeployment": true,
                "enableSoftDelete": true,
                "softDeleteRetentionInDays": 7,
                "enableRbacAuthorization": true,
                "vaultUri": "[concat('https://', parameters('vaults_keyvaultenv_name'), '.vault.azure.net/')]",
                "provisioningState": "Succeeded",
                "publicNetworkAccess": "Enabled"
            }
        },
        {
            "type": "Microsoft.KeyVault/vaults/secrets",
            "apiVersion": "2024-04-01-preview",
            "name": "[concat(parameters('vaults_keyvaultenv_name'), '/AdminP')]",
            "location": "canadacentral",
            "dependsOn": [
                "[resourceId('Microsoft.KeyVault/vaults', parameters('vaults_keyvaultenv_name'))]"
            ],
            "properties": {
                "attributes": {
                    "enabled": true
                }
            }
        }
    ]
}